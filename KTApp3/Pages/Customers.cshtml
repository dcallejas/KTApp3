@page
@model KTApp3.Pages.CustomersModel
@{
    ViewData["Title"] = "Customers";
}
<h1>From the data api</h1>
<table class="table">
    <tr>
        <th>Id</th>
        <th>Name</th>
        <th>VAT</th>
        <th>Enabled</th>
    </tr>
    @foreach (var item in Model.CustomersList)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.VAT</td>
            <td>@item.Enabled</td>
        </tr>
    }
</table>

